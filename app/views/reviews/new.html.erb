<div class="review-new-wrapper">
  <div class="bkRGBA">
    <div class="container">
      <div class="row">
        <div class="col-md-offset-3 col-md-6 users-new-container">
          <h1 class="text-center">REVIEW</h1>
          <%= form_for @review do |f| %>
          <div class="form_group">
            <%= f.label:review %>
            <%= f.text_area :review ,class: "form_control" %>
          </div>
          <%= f.submit "投稿", class:"btn-black btn-block" %>
          <% end %>
        </div><!--users-new-container-->
      </div><!--row-->
    </div><!--container-->
  </div><!--bkrgb-->
</div><!--new-wrapper-->